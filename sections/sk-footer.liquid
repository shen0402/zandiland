        <script>
          $(document).ready(function() {
              $(".nav-dropdown-toggle").on('click', 
        
        
              function() {
        
                
                if ($(this).siblings(".nav-dropdown-menu").hasClass('w--open')==true) /* Chaeck if the arrow is rotated */
                {
                  $(this).siblings(".nav-dropdown-menu").removeClass("w--open");
        
                }
                else
                $(this).siblings(".nav-dropdown-menu").addClass("w--open");
              },
        
        
        
              
              
              );
          });
        
          $(document).ready(function() {
              $(".nav-dropdown-toggle").on('click', 
        
        
              function() {
                
                if ($(this).children(".mobile-nav-arrow").hasClass('rotate')==false)
                {
                  $(this).children(".mobile-nav-arrow").addClass("rotate");
                }
                else
                  $(this).children(".mobile-nav-arrow").removeClass("rotate");
              },
              );
          });
        
            </script>
        



























