    <div class="nav-container w-container">
      <div class="nav-flex-row-outer">
        <div class="nav-meta-section">
          <div class="nav-meta-flex-row">
            <a href="/account/login" class="nav-meta-flex-link w-inline-block"><img src="//cdn.shopify.com/s/files/1/0004/5829/3310/files/Account-New-solid.png" loading="lazy" alt="User" class="nav-meta-flex-img"></a>
          </div>
        </div>
        <div class="nav-logo-wrapper-mobile">
          <a href="/" class="nav-logo-link-mobile w-inline-block">
            <img src="//cdn.shopify.com/s/files/1/0004/5829/3310/files/Curved_catlogo-BrightPink_1.png?v=1675189018" loading="lazy" alt="ZandiLand" class="nav-logo-img-mobile">
